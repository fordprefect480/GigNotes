<Application 
    x:Class="GigNotes.App"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone" 
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"   
    xmlns:cal="clr-namespace:Caliburn.Micro;assembly=Caliburn.Micro"
    xmlns:gn="clr-namespace:GigNotes">

    <Application.Resources>
        <system:String x:Key="IsoDbConnection">isostore:/GigNotes.sdf</system:String>
        <cal:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <gn:Bootstrapper x:Key="bootstrapper"></gn:Bootstrapper>
    </Application.Resources>
</Application>